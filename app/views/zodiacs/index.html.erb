<div class="page-header">
  <h1>List of Zodiacs</h1>
</div>

<div class="row">
  <div class="col-md-12">
    <ul class="list-group">
      <% @zodiac_hashes.each do |zodiac_hash| %>
        <li class="list-group-item">
          <a href="/zodiacs/<%= zodiac_hash[:sign] %>">
            <%= zodiac_hash[:sign].capitalize %>
          </a>: <%= zodiac_hash[:horoscope] %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
