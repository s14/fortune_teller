<div class="page-header">
  <h1>List of Zodiacs</h1>
</div>

<div class="row">
  <div class="col-md-12">
    <ul class="list-group">
      <% @zodiacs.each do |zodiac| %>
        <li class="list-group-item">
          <a href="/zodiacs/<%= zodiac.sign %>" class="label label-primary">
            <%= zodiac.sign.capitalize %>
          </a>
          &nbsp;<%= zodiac.horoscope %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
